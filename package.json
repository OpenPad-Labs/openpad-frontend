    "stylelint-scss": "^4.3.0",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "**/*.{css,scss,sass,html}": [
      "stylelint --fix"
    ],
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0 --no-ignore .eslintrc.js --fix",
      "prettier -w ."
    ],
    "**/*.{json,css,scss,md}": [
      "prettier -w ."
    ]
